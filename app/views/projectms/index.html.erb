<p id="notice"><%= notice %></p>

<h1>Listing Projects</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>	
      <th>Status</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projectms.each do |projectm| %>
      <tr>
        <td><%= projectm.name %></td>
        <td><%= projectm.status %></td>
        <td><%= projectm.description %></td>
        <td><%= link_to 'Show', projectm %></td>
        <td><%= link_to 'Edit', edit_projectm_path(projectm) %></td>
        <td><%= link_to 'Destroy', projectm, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Projectm', new_projectm_path %>
